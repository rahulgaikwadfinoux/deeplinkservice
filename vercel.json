{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/\\.well-known/(.*)",
            "dest": "/.well-known/$1"
        }
    ],
    "rewrites": [
        {
            "source": "/.well-known/(.*)",
            "destination": "/.well-known/$1"
        }
    ]
}