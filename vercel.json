{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/\\.well-known/assetlinks.json",
            "dest": "/\\.well-known/assetlinks.json"
        },
        {
            "src": "/(.*)",
            "dest": "/\\.well-known/assetlinks.json"
        }
    ]
}